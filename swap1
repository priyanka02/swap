public class swap
{
  public static void main(String args[])
  {
    Scanner sc=new Scanner(System.in);
    int a,b,temp;
    System.out.println("Enter two no's");
    a=sc.nextInt();
    b=sc.nextInt();
    temp=a;
    a=b;
    b=temp;
    System.out.println("a="+a);
    System.out.println("b="+b);
  }
}
